{
    "manifest_version": 2,
    "name": "Google Keep",
    "description": "Chrome App to use Google Keep in a window.",
    "version": "1",
    "icons": {
        "48": "icon_48.png",
        "144": "icon_144.png",
        "192": "icon_192.png"
    },
    "permissions": [
        "webview"
    ],
    "app": {
        "background": {
            "scripts": [
                "background.js"
            ]
        }
    },
    "minimum_chrome_version": "34"
}
